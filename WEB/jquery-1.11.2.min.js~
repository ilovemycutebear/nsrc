$(document).ready(function (){

function gtJson(){
 $.getJSON('order.php', function(jd) {
             $('#temp').html(jd.Temp +' &degC');
             $('#humi').html(jd.Humi + ' %');
             $('#Wspd').html(jd.Wspd + ' m/s');
             $('#Wndr').html(jd.Wdir +' &deg '+jd.direc);
             $('#slr').html(jd.Solar +' w/m&sup2');
             $('#press').html(jd.Press+ ' HPa');
             $('#rain').html(jd.R15 +' mm');
             $('#battv').html(jd.Batt+' V');		
             $('#dt').html(jd.Date);		
             $('#tm').html(jd.Time);
		
          });
		setTimeout(gtJson,20000);
		console.log("ya!");
	}
function gtrnJson(){
 $.getJSON('dailyrain.php', function(je) {
             $('#ttlrain').html(je.Rtotal + ' mm');
            
          });
		setTimeout(gtrnJson,30000);
		console.log("hea!");
	}
	gtrnJson();
	gtJson();
});
